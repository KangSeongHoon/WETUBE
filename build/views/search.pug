extends layouts/main
include mixins/videoBlock

block content 
    .serch_header
        h3 Searching for: #{searchingBy}
    .search_videos
        if videos.length ===0
            h5 NO Video Found
        each item in videos
            +videoBlock({
                title:item.title,
                views:item.views,
                videoFile:item.videoFile,
                id:item.id
            })