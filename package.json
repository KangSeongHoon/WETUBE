    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fs": "0.0.1-security",
    "get-blob-duration": "^1.1.0",
    "helmet": "^3.15.0",
    "mongo": "^0.1.0",
    "mongodb": "^3.5.4",
    "mongoose": "^5.9.6",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
